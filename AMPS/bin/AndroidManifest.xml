        <activity
            android:name="com.example.amps.MainTabMenu"
            android:label="@string/title_activity_main_tab_menu" >
        </activity>
        <activity
            android:name="com.example.amps.ProfileActivity"
            android:label="@string/title_activity_profile" >
        </activity>
        <activity
            android:name="com.example.amps.ProfileAccountFragment"
            android:label="@string/title_fragment_profile_account" >
        </activity>
        <activity
            android:name="com.example.amps.ProfilePasswordFragment"
            android:label="@string/title_fragment_profile_password" >
