        </activity>
        <activity
            android:name="com.example.amps.SearchFragment"
            android:label="@string/title_fragment_search" >
        </activity>
        <activity
            android:name="com.example.amps.ReportFragment"
            android:label="@string/title_fragment_report" >
        </activity>
        <activity
            android:name="com.example.amps.ReportActivity"
            android:label="@string/title_activity_report" >
